algoritmo "semnome"
// Função :
// Autor :
// Data : 20/06/2022
// Seção de Declarações 
var
num: inteiro
inicio
escreval("digite um número:  ")
LEIA(NUM)

SE NUM < 0 ENTAO
 //VERDADEIRO
 escreval("NEGATIVO")
 senao
  //FALSO
  se num > 0 entao
  
  //VERDADEIRO
  escreval("POSITIVO")
  senao
  //FALSO
  escreval("zero")
  fimse
 fimse






fimalgoritmo
